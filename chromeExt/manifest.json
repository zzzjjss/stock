{
  "manifest_version": 2,

  "name": "Image Switcher",
  "description": "Change all images in the tab!",
  "version": "1.0",
"content_scripts": [
  {
    "matches": [
      "https://www.jiumodiary.com/"
    ],
    "js": ["js/jquery.min.js","js/content.js","js/bookName.js"]
  }
],
  "icons": {
    "48": "img/icon.png"
  },
"browser_action": {
    "default_icon": "img/icon.png",
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab"]
}